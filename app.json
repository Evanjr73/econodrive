{
  "expo": {
    "name": "econodrive",
    "slug": "econodrive",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/EconoDrive.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00C0EB"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/EconoDrive.png",
        "backgroundColor": "#00C0EB"
      },
      "package": "com.evan_junior73.econodrive"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/EconoDrive.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4013254471054866~7254560753",
          "iosAppId": "ca-app-pub-xxxxxxxx~xxxxxxxx"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4",
            "useFrameworks": "static"
          }
        }
      ],
      "expo-font"
     
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "79d34c50-e566-4cc7-a826-76ace72f6a58"
      }
    }
  }
}
